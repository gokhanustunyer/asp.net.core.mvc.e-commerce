@model List<Tuple<string, string>>


<div id="container">
    <div id="inner_container">
        <div id="imgs_container">
            @for (int i = 0; i < Model.Count; i++)
            {   
                <div class="img_container">
                    <p style="margin-bottom:0;width:100%;background-color:black;color:white;text-align:center;padding:.75rem;font-size:.9rem">
                        @Html.Raw(Model[i].Item1)</p>
                </div>
            }
        </div>
    </div>
</div>